	<Message description="Feature: ThreeOrMoreScripts" key="{2b4b7c3b-e22a-4edd-abf9-2da66c028f3f}" value="3+ concurrently running scripts" />
	<Message description="Feature: ModbusSend" key="{80377ad6-d55a-4432-bccf-1eb720bbb053}" value="MODBUS Send" />
	<Message description="Feature: ModbusScripts" key="{6e24a8d6-63e6-4b65-a0c5-eec56ffd76bc}" value="MODBUS Scripting" />
