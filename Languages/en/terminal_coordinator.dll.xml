	<Item text="Device"/>
  </Enumeration>

	<Message key="Devices" description="Tool window: Devices" />

	<Message key="toolbar.Devices" description="Toolbar: Devices" value="Devices" />

	<!-- LM Dialog -->
	<Enumeration key="LicenseState" description="Current license state">
		<Item text="Free, trial period has not been started." />
		<Item text="Free, trial period ends in [[[b{days}]]] days.">
			<Param name="days"/>
		</Item>
		<Item text="Free, trial period is over." />
		<Item text="[[[b{edition}]]]\r\nRegistered to [[[i{user}]]]">
			<Param name="edition"/>
			<Param name="user"/>
		</Item>
		<Item text="TODO: Upgrade plan expired" />
	</Enumeration>

	<Message key="ComNonComSuffix" description="Commercial or non-commercial license suffix" value="\r\n{comtype}">
		<Param name="comtype"/>
	</Message>

	<Enumeration key="ComNonCom" description="Commercial and non-commercial license strings">
		<Item text="Non-commercial use only"/>
		<Item text="Commercial usage allowed"/>
	</Enumeration>

	<Message key="LicenseFileFilter" description="License file open dialog filter" value="License Files|*.termlic|All Files|*||">
		<Validate rule="OpenFileDialogFilter"/>
	</Message>
	<Message key="License has been successfully installed." description="License has been successfully installed."/>
	<Message key="There was an error installing a license file." description="There was an error installing a license file." value="There was an error installing a license file. The file might be corrupted or expired. Please log into the My Account section and download new license file."/>

<!-- Welcome nag screen -->
	<Message key="Trial dialog title: free" description="Trial dialog: title in free edition" value="Get Extended Functionality"/>
	<Message key="Trial text: free" description="Trial dialog: text in free edition" value="You are currently using feature-limited [[[bFREE VERSION]]] of the product.\nYou may either continue using limited version or try extended functionality for 14 days first, then switch back to free."/>
	<Message key="Trial link text: free" description="Trial link text: free" value="Continue with limited features" />

	<Message key="Trial dialog title: trial" description="Trial dialog: title in trial" value="Get Extended Functionality"/>
