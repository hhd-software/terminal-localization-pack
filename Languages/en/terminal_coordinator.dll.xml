		<Param name="days" />
	</Message>
	<Message key="Trial link text: trial" description="Trial link text: trial" value="Continue evaluation" />

	<Message key="Trial dialog title: expired" description="Trial dialog: title in expired edition" value="Get Extended Functionality"/>
	<Message key="Trial text: expired" description="Trial dialog: text in expired edition" value="You are currently using feature-limited [[[bFREE VERSION]]] of the product.\nYou may either continue using limited version or upgrade to the extended version. Purchasing extended functionality will remove this screen." />
	<Message key="Trial link text: expired" description="Trial link text: expired" value="Continue with limited features" />

	<!-- Licensing -->
	<Message key="NagFreeTitle" description="Title: Feature Not Available" value="Feature not Available"/>
	<Message key="Close this notification" description="Link text: Close this notification" value="Close this notification"/>
	<Message key="Try extended version" description="Button text: Try extended version"/>
	<Message key="Purchase extended version" description="Button text: Purchase extended version" value="Get extended version"/>

	<Message key="FreeVersionNagText1" description="Feature is not included in the Free version (trial not started)" value="{feature} feature is not included in the Free version.\n\nYou may continue with limited features of Free version or you may try the extended functionality for FREE during 14 days.">
		<Param name="feature"/>
	</Message>
	<Message key="FreeVersionNagText2" description="Feature is not included in the Free version (trial finished)" value="{feature} feature is not included in the Free version.\n\nYou may continue with limited features of Free version or you may upgrade to the extended version.">
		<Param name="feature"/>
	</Message>
