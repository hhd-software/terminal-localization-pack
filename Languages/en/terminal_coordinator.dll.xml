		<Param name="days" />
	</Message>
	<Message key="Trial link text: trial" description="Trial link text: trial" value="Continue evaluation" />

	<Message key="Trial dialog title: expired" description="Trial dialog: title in expired edition" value="Get Extended Functionality"/>
